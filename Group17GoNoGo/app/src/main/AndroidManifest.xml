<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.group17gonogo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Group17GoNoGo">
        <activity
            android:name=".TestResultPopUp"
            android:label="@string/title_activity_test_result_pop_up"
            android:theme="@style/result_popup">
        </activity>

        <activity android:name=".RegistrationActivity"></activity>
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:label="MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GoNoGoActivity"
            android:label="Go No-Go Test"></activity>

        <activity android:name=".InstructionsActivity"/>
    </application>

</manifest>